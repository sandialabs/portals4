<?xml version="1.0"?>
<test>
      <desc>Test PtlPut with good parameters to LE</desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="16384" ni_opt="NO_MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>
                    
                    <if rank="0">
                      <ptl_md md_data="0xf0f0f0f0" md_length="16384" type="UINT">
                        <barrier/>
                        <ptl_put ack_req="ACK" length="16384" target_id="1"/>
                        <ptl_eq_poll time="200">
                          <check event_type="SEND"/>
                        </ptl_eq_poll>
                        <ptl_eq_poll time="200">
                          <check event_type="ACK"/>
                        </ptl_eq_poll>
                        <barrier/>
                      </ptl_md>
                    </if>
                    
                    <if rank="1">
                      <ptl_le le_data="0x00000000" le_length="16384" le_opt="OP_PUT EVENT_LINK_DISABLE" type="UINT">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="PUT"/>
                        </ptl_eq_poll>
                        <check le_data="0xf0f0f0f0" length="16384" type="UINT"/>
                        <barrier/>
                      </ptl_le>
                    </if>
                    
                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>