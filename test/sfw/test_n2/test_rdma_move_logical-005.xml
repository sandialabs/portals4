<?xml version="1.0"?>
<test>
      <desc>Test PtlGet with good parameters from LE</desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="16384" ni_opt="NO_MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>
                    
                    <if rank="0">
                      <ptl_md md_data="0xffffffff" md_length="16384" type="UINT">
                        <barrier/>
                        <ptl_get length="16384" target_id="1"/>
                        <ptl_eq_poll time="200">
                          <check event_type="REPLY"/>
                        </ptl_eq_poll>
                        <check length="16384" md_data="0x0f0f0f0f" type="UINT"/>
                        <barrier/>
                      </ptl_md>
                    </if>
                    
                    <if rank="1">
                      <ptl_le le_data="0x0f0f0f0f" le_length="16384" le_opt="OP_GET EVENT_LINK_DISABLE" type="UINT">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="GET"/>
                        </ptl_eq_poll>
                        <barrier/>
                      </ptl_le>
                    </if>
                    
                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>