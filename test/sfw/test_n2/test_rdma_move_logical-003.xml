<?xml version="1.0"?>
<test>
      <desc>Test PtlGet with good parameters from ME</desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="8192" ni_opt="MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>
                    
                    <if rank="0">
                      <ptl_md md_length="8192" me_data="0x00000000" type="UINT">
                        <barrier/>
                        <ptl_get length="8192" match="0x5555" target_id="1"/>
                        <ptl_eq_poll time="200">
                          <check event_type="REPLY"/>
                        </ptl_eq_poll>
                        <check length="8192" md_data="0x5a5a5a5a" type="UINT"/>
                        <barrier/>
                      </ptl_md>
                    </if>
                    
                    <if rank="1">
                      <ptl_me me_data="0x5a5a5a5a" me_length="8192" me_match="0x5555" me_opt="OP_GET EVENT_LINK_DISABLE" type="UINT">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="GET"/>
                        </ptl_eq_poll>
                        <barrier/>
                      </ptl_me>
                    </if>
                    
                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>