<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_GT/CHAR</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ni ni_opt="MATCH LOGICAL">
          <ptl_pt>
            <ptl_me me_data="0x69" me_match="0x22e9e738" me_opt="OP_PUT OP_GET" type="CHAR">
              <ptl_md md_data="0xb0" type="CHAR">
                <ptl_md md_data="0x0c" type="CHAR">
                  <ptl_swap atom_op="CSWAP_GT" atom_type="CHAR" length="1" match="0x22e9e738" operand="0x10" target_id="SELF"/>
                  <msleep count="50"/>
                </ptl_md>
                <check length="1" md_data="0xb0" type="CHAR"/>
                <check length="1" md_data="0xb0" offset="1" type="CHAR"/>
              </ptl_md>
              <check length="1" me_data="0x69" type="CHAR"/>
              <check length="1" me_data="0x69" offset="1" type="CHAR"/>
            </ptl_me>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>
