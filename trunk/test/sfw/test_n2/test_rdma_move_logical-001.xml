<?xml version="1.0"?>
<test>
      <desc>Test PtlPut 32K with good parameters to ME</desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="32768" ni_opt="MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>

                    <if rank="0">
                      <ptl_md md_data="0xa5a5a5a5" md_length="32768" type="UINT">
                        <barrier/>
                        <ptl_put ack_req="ACK" length="32768" match="0x5555" target_id="1"/>
                        <ptl_eq_poll time="200">
                          <check event_type="SEND"/>
                        </ptl_eq_poll>
                        <ptl_eq_poll time="200">
                          <check event_type="ACK"/>
                        </ptl_eq_poll>
                        <barrier/>
                      </ptl_md>
                    </if>

                    <if rank="1">
                      <ptl_me me_data="0x00000000" me_length="32768" me_match="0x5555" me_opt="OP_PUT EVENT_LINK_DISABLE" type="UINT">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="PUT"/>
                        </ptl_eq_poll>
                        <check length="32768" me_data="0xa5a5a5a5" type="UINT"/>
                        <barrier/>
                      </ptl_me>
                    </if>

                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
              <barrier/>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>