<?xml version="1.0"?>
<test>
      <desc>Test PtlGet with good parameters LE and MD IOVEC of 8 </desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="8192" ni_opt="NO_MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>
                    
                    <if rank="0">
                      <ptl_md md_data="0x00000000" md_length="8" md_opt="IOVEC" type="UINT">
                        <barrier/>
                        <ptl_get length="8192" target_id="1"/>
                        <ptl_eq_poll time="200">
                          <check event_type="REPLY"/>
                        </ptl_eq_poll>
                        <check length="8192" md_data="0x84218421" type="UINT"/>
                        <barrier/>
                      </ptl_md>
                    </if>
                    
                    <if rank="1">
                      <ptl_le le_data="0x84218421" le_length="8" le_opt="IOVEC OP_GET EVENT_LINK_DISABLE" type="UINT">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="GET"/>
                        </ptl_eq_poll>
                        <barrier/>
                      </ptl_le>
                    </if>
                    
                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>