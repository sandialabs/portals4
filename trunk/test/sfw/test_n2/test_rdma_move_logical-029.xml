<?xml version="1.0"?>
<test>
      <desc>Test PtlGet with good parameters MD IOVEC of 8*8 </desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="8192" ni_opt="MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>

                    <if rank="0">
                      <ptl_md iovec_len="8" md_data="0xffffffff" md_length="16" md_opt="IOVEC" type="UINT">
                        <barrier/>
                        <ptl_get length="64" match="0x5555" target_id="1"/>
                        <ptl_eq_poll time="200">
                          <check event_type="REPLY"/>
                        </ptl_eq_poll>
                        <check length="64" md_data="0x0a5f0a5f" type="UINT"/>
                        <check length="64" md_data="0xffffffff" offset="64" type="UINT"/>
                        <barrier/>
                      </ptl_md>
                    </if>

                    <if rank="1">
                      <ptl_me me_data="0x0a5f0a5f" me_length="8192" me_match="0x5555" me_opt="OP_GET EVENT_LINK_DISABLE" type="UINT">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="GET"/>
                        </ptl_eq_poll>
                        <barrier/>
                      </ptl_me>
                    </if>

                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </test>